<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.fallsrisk" >

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MorseFalls"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogIn"
            android:label="@string/title_activity_log_in"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".ChooseScale"
            android:label="@string/title_activity_choose_scale"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".FRAT"
            android:label="@string/title_activity_frat"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Efficacy"
            android:label="@string/title_activity_efficacy"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Tinetti"
            android:label="@string/title_activity_tinetti"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
